#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/node_modules:/mnt/c/Users/hggmarks/Documents/gama/node_modules:/mnt/c/Users/hggmarks/Documents/node_modules:/mnt/c/Users/hggmarks/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/react-router@5.2.0/node_modules/react-router/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/react-router@5.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/node_modules:/mnt/c/Users/hggmarks/Documents/gama/node_modules:/mnt/c/Users/hggmarks/Documents/node_modules:/mnt/c/Users/hggmarks/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/react-router@5.2.0/node_modules/react-router/node_modules:/mnt/c/Users/hggmarks/Documents/gama/client-server-example/packages/server/src/node_modules/.pnpm/react-router@5.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
